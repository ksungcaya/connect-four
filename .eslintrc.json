{
  "extends": [
    "airbnb-base",
    "plugin:vue/recommended"
  ],
  "rules": {
    "newline-before-return": ["error"],
    "no-underscore-dangle": 0,
    "class-methods-use-this": 0,
    "linebreak-style": 0,
    "validateLineBreaks": 0,
    "import/extensions": ["error", "always", {
      "js": "never",
      ".vue": "never"
    }]
  },
  "settings": {
    "import/resolver": {
      "node": {
        "extensions": [".js", ".jsx", ".vue"]
      }
    }
  }
}
